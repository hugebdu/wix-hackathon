<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
         version="3.0">

    <servlet>
        <servlet-name>SettingsServlet</servlet-name>
        <servlet-class>com.wixpress.googleadsense.SettingsServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>AdUnitServlet</servlet-name>
        <servlet-class>com.wixpress.googleadsense.AdUnitServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>AdUnitServlet</servlet-name>
        <url-pattern>/adunit/*</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>SettingsServlet</servlet-name>
        <url-pattern>/settings/*</url-pattern>
    </servlet-mapping>

</web-app>